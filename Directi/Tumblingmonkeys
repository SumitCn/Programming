#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;

int findroot(int ar[],int br[],int i)
 { map<ll,ll> mp;
    while(br[i]!=i&&mp[i]==0)
     { 
       mp[i]=1;
       i=ar[i];
       
      }
   return i;
  }
int main()
{
 int t;
 cin>>t;
 while(t--)
 { 
   int n;
   cin>>n;
  int ar[10001];
   for(int i=1;i<=n;i++)
    { cin>>ar[i];
    }  
  int br[10001]={0};
   for(int i=1;i<=n;i++)
    {
      int x=findroot(ar,br,i);
      br[i]=x;
        }
  int cr[10001]={0};
  for(int i=1;i<=n;i++)
    { cr[br[i]]++;
        }
  int ans=0;
  for(int i=0;i<=n;i++)
    ans=max(ans,cr[i]);
  cout<<ans<<"\n";
  }
return 0;
}
