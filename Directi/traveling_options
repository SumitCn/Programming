#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main()
{
int t , temp ;
cin>>t;
vector<ll> v;
for(int i=0;i<t;i++)
 { cin>>temp;
  v.push_back(temp);
}
sort(v.begin(),v.end());
string bb;
cin>>bb;
int count=0;
if(bb=="NIL")
 cout<<"0";
else
{
int b=stoi(bb);
for(int i=0;i<t;i++)
  { if(v[i]>b)
     { break; }
    else if(v[i]<=b)
       {count++;b-=v[i];}

    }
 
  if(count==0)
  cout<<"-1";
 else 
   cout<<count;
 }
  
 


return 0;
}
